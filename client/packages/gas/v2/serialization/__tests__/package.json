{
  "name": "@gas/serialization-tests",
  "version": "1.0.0",
  "description": "Unit tests for GAS v2 serialization system",
  "scripts": {
    "test": "jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watch",
    "test:coverage": "jest --config jest.config.js --coverage",
    "test:ci": "jest --config jest.config.js --ci --coverage --watchAll=false",
    "test:verbose": "jest --config jest.config.js --verbose",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --config jest.config.js --runInBand"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  }
}